{
  "compilerOptions": {
    "target": "es5",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "examples"]
}
